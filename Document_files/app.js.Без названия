
$( document ).ready(function() {
    $('.carousel').carousel({touch:true});




    $(".js-range-slider").ionRangeSlider({

        type: "double",
        min: 1,
        max: 300,
        from: 10,
        to: 200,
        skin: "round",
        step: 1,
        prettify_enabled: true,
        prettify_separator: ",",
        prefix: "$",
        postfix: "K",
    });

    $('.car-body-slider').slick({
        infinite: true,
        dots: false,
        slidesToShow: 6,
        slidesToScroll: 3,
        variableWidth: true,
        responsive: [
            {
                breakpoint: 992,
                settings: {
                    arrows:false,
                    slidesToShow: 3,
                }
            },

        ]

    });
    $('.gallery-slider').slick({
        infinite: false,
        dots: true,
        arrows:false
    });

    $('.testimonials').slick({

        dots: true,
        infinite: true,
        slidesToShow: 3,
        slidesToScroll: 1,
        responsive: [
            {
                breakpoint: 992,
                settings: {

                    slidesToShow: 2,
                }
            },
            {
                breakpoint: 768,
                settings: {
                    arrows:false,
                    slidesToShow: 1,
                }
            },

        ]
    });
    $('.partners-slider').slick({
        infinite: true,
        arrows:false,
        dots: false,
        slidesToShow: 6,
        slidesToScroll: 3,
        variableWidth: true,

    });



});